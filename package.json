{
  "name": "outfund-website",
  "version": "1.0.0",
  "description": "Built with 11ty",
  "scripts": {
    "server": "npx @11ty/eleventy --serve --quiet",
    "styles:dev": "sass --embed-source-map --source-map-urls='absolute' './src/styles/main.scss' './dist/main.css'",
    "watch:styles": "onchange './src/styles/**/*' -- yarn styles:dev",
    "dev": "npm-run-all --parallel server watch:*",
    "clean": "rimraf './dist'",
    "clean-all": "rimraf './dist' & rimraf './node_modules'"
  },
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1",
    "sass": "^1.62.1",
    "onchange": "^7.1.0"
  }
}
