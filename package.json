{
  "name": "outfund-website",
  "version": "1.0.0",
  "description": "Built with 11ty",
  "scripts": {
    "server": "npx @11ty/eleventy --serve --quiet",
    "styles:dev": "stylus --watch --sourcemap  src/styles/main.styl --out dist/main.css",
    "watch:styles": "onchange './src/styles/**/*' -- yarn styles:dev",
    "develop": "npm-run-all --parallel server watch:*",
    "clean": "rimraf './dist'",
    "clean-all": "rimraf './dist' & rimraf './node_modules'"
  },
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "rimraf": "^5.0.1",
    "sass": "^1.62.1",
    "stylus": "^0.59.0"
  },
  "dependencies": {
    "rupture": "^0.7.1"
  }
}
