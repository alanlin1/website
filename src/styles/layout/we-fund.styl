
#weFund
  // background-color #afa
  .section__header
    text-align center
    margin-bottom 32px

  .section__content
    display flex
    flex-wrap wrap
    gap 30px
  .brand
    background-color #aaa
    max-width 500px
    width 100%
    height 380px
    border-radius 4px
    padding 48px 40px
    display flex
    flex-direction column
    overflow hidden

    background-repeat no-repeat
    background-position 100% 100%

    &__logo
      height 40px
      background-repeat no-repeat
      background-size contain
      margin-bottom 24px

    &__text
      font-size 22px
      line-height 36px
      max-width 280px
      margin-bottom auto

    &__founderName
      font-weight 700
      font-size 20px
      line-height 24px

    &__position
      font-size 16px
      line-height 24px

    &__lemonade, 
    &__supercharging
      max-width 440px

    &__lemonade
      background-color #EDD8DB
      background-image url('founder-lemon.jpg')
      background-size 190px 289px
      
      .brand__logo
        background-image url('data:image/png;base64,')


    &__feel
      background-color #EDE3D9
      background-image url('founder-feel.jpg')
      background-size 274px 317px
      .brand__logo
        background-image url('data:image/png;base64,')
    
    &__onto
      background-color #DCDED4
      background-image url('founder-onto.jpg')
      background-size 226px 257px
      .brand__logo
        background-image url('data:image/png;base64,')
      
    
    &__supercharging
      background-color #E1ECF3
      align-items flex-start
      justify-content center
      &__wrapper
        width: min-content;
        margin-left auto
        margin-right auto
        display flex
        flex-direction column
        align-items flex-start
        gap 32px
        justify-content center
        h3
          font-weight 700
          font-size 40px
          line-height 44px

      
